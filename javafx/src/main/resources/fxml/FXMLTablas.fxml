          <TableColumn prefWidth="75.0" text="nombre" />
          <TableColumn prefWidth="75.0" text="stock" />
        </columns>
      </TableView>
      <Button layoutX="248.0" layoutY="72.0" mnemonicParsing="false" onAction="#clickAddProducto" text="Button" />
